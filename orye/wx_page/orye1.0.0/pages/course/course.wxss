.mutiTitle{
  width: 100%
}
.mutiTitle image{
  width: 100%;
  height: 340rpx;
}
.cbtn{
    width: 40%;
    color:green;
    display:block;
    margin:0 auto;
    text-align: center;
}
.cbtn text{
  margin-top: 10rpx;
  font-size: 25rpx;
  text-decoration:underline;
}
.cbtn checkbox .wx-checkbox-input {
  width: 30rpx;
  height: 30rpx;
}
.teacher_title{
  line-height: 60rpx;
  font-weight: bold;
  padding-left: 10rpx;
}
.teacher{
  width: 100%;
  display: flex;
  flex-direction: row;
  height: auto;
  padding-bottom: 10rpx;
}
.teacher .cont{
  width: 60%;
  border-top: 1rpx solid #ccc;
  padding: 10rpx;
}
.teacher .place1{
  height: 55rpx;
  font-size: 28rpx;
}
.teacher .image{
  width: 40%;
  text-align: right;
}
.teacher .image image{
  width: 80%;
}
.time{
  line-height: 55rpx;
  font-size: 28rpx;
  border-style:solid none none none;
  border-width: 1rpx;
  border-color: #ccc;
}
.place{
  line-height: 55rpx;
  font-size: 28rpx;
  border-top: 1rpx solid #ccc;
}
.time .text{
  padding: 10rpx;
}
.place .text{
  padding: 10rpx;
}
.remark{
  font-size: 28rpx;
  line-height: 55rpx;
  border-style:solid none none none;
  border-width: 1rpx;
  border-color: #ccc;
}
.remark .text{
  padding: 10rpx;
}
.label{
  margin-bottom: 25rpx;
  line-height: 55rpx;
  font-size: 28rpx;
  border-style:solid none solid none;
  border-width: 1rpx;
  border-color: #ccc;
}
.label .text{
  padding: 10rpx;
}
.yyBtn{
  background: #349f73;
  color: #000;
}